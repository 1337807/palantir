<!DOCTYPE html>
<html>
<head>
  <title>Palantir</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= yield :header %>
</head>
<body>

<div class='jumbotron'>
  <h1>Palantir</h1>
  <div class='container'>
    <div id='content'>
      <%= link_to('Sign out', destroy_user_session_path, method: :delete) if current_user %>
      <%= yield %>
    </div>
  </div>
</div>

</body>
</html>
